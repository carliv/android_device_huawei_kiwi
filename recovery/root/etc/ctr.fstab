# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

# mount point	fstype    device    	[device2]    fs_options    [fs_options2]    lun

/boot	emmc	/dev/block/bootdevice/by-name/boot
/cache	ext4	/dev/block/bootdevice/by-name/cache
/recovery	emmc	/dev/block/bootdevice/by-name/recovery
/system	ext4	/dev/block/bootdevice/by-name/system
/data	ext4	/dev/block/bootdevice/by-name/userdata
/custpack	ext4	/dev/block/bootdevice/by-name/cust
/internal_sd	datamedia	/dev/null
/sdcard	vfat	/dev/block/mmcblk1p1	/dev/block/mmcblk1	lun=sys/class/android_usb/android0/f_mass_storage/lun/file
/usbdisk	auto	/dev/block/sda1	/dev/block/sda

/frp	emmc	/dev/block/bootdevice/by-name/config
/persistent	emmc	/dev/block/bootdevice/by-name/pad
/log	vfat	/dev/block/bootdevice/by-name/log
